{
    "name": "msucevan/vapor-tenant-buckets-new",
    "description": "Automatically Provision AWS S3 Buckets for tenants.",
    "keywords": [
        "msucevan",
        "tenant-buckets",
        "Tenancy For Laravel",
        "tenancyforlaravel"
    ],
    "homepage": "https://github.com/syn6UK/tenant-buckets-vapor",
    "license": "MIT",
    "authors": [
        {
            "name": "Shashwat Mishra",
            "email": "shashwat@vidwan.co",
            "role": "Developer"
        },
        {
            "name": "Robert Lane",
            "email": "rob@updev.agency",
            "role": "BugFixer"
        }
    ],
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "~3.0",
        "stancl/tenancy": "^3.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Lanos\\TenantBuckets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lanos\\TenantBuckets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
